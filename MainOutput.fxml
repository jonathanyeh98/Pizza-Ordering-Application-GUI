<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="mainLayout" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MainOutputController">
   <top>
      <Pane fx:id="primaryStage" prefHeight="600.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="Output" layoutY="116.0" prefHeight="484.0" prefWidth="500.0" />
            <Button fx:id="ClearOrder" layoutX="170.0" layoutY="48.0" mnemonicParsing="false" onAction="#clearOrder" text="Clear Order" />
            <Button fx:id="Back" layoutX="275.0" layoutY="48.0" mnemonicParsing="false" onAction="#closeWindow" text="Back" />
         </children>
      </Pane>
   </top>
</BorderPane>
